	private async gatherManufacturingInfo(
		processType: ProcessType,
		machineEfficiency: number,
		density: number
	): Promise<ProcessInfoDto> {
		logger.info('üì• Gathering Manufacturing Info from UI...')

		// ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
		// 1Ô∏è‚É£ Manufacturing fields
		// ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
		const [
			machineHourRate, // 0
			machineEfficiencyUI, // 1
			lowSkilledLaborRatePerHour, // 2
			noOfLowSkilledLabours, // 3
			noOfSkilledLabours, // 4
			skilledLaborRatePerHour, // 5
			electricityUnitCost, // 6
			powerConsumption, // 7
			yieldPer, // 8
			annualVolume, // 9
			setUpTime, // 10
			qaOfInspectorRate, // 11
			inspectionTime, // 12
			samplingRate, // 13
			netMaterialCost, // 14
			cycleTime, // 15
			totalWeldLength, // 16
			weldLegLength, // 17
			weldElementSize, // 18
			PartReorientationTime, // 19
			partProjectedArea, // 20
			cuttingLength, // 21
			travelSpeed, // 22
			UnloadingTime, // 23
			semiAutoOrAuto, // 24
			netWeight, // 25
			densityUI, // 26
			dryCycleTime, // 27
			RequiredVoltage, // 28
			selectedVoltage, // 29
			passesLocator // 30
		] = await this.readManufacturingInputs()


		// ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
		// 2Ô∏è‚É£ Lot size
		// ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
		const lotSize = await this.getInputNumber(this.page.LotsizeNos)

		if (
			lotSize === 1 &&
			[ProcessType.WeldingCleaning, ProcessType.WeldingPreparation].includes(processType)
		) {
			logger.warn('‚ö†Ô∏è Lot size = 1 (possible fallback)')
		}

		// ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
		// 3Ô∏è‚É£ Material & Part Info (tab-safe)
		// ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
		const [
			netPartWeight,
			partComplexity,
			materialTypeName,
			materialDims
		] = await Promise.all([
			this.getNetWeight(),
			this.getPartComplexity(),
			this.getMaterialType(),
			this.getMaterialDimensionsAndDensity()
		])

		const { length, width, height } = materialDims

		// ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
		// 4Ô∏è‚É£ Weld Sub-Materials & Sub-Processes
		// ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
		const [weldSubMaterials, uiSubProcesses] = await Promise.all([
			Promise.all([1, 2].map(i => this.collectWeldSubMaterial(i as 1 | 2))),
			this.collectSubProcesses()
		])

		const validSubMaterials = weldSubMaterials.filter(Boolean) as WeldSubMaterialUI[]

		const coreCostDetails = validSubMaterials.map((weld, i) => {
			const sub = uiSubProcesses[i]
			return {
				coreWeight: weld.weldElementSize,
				coreHeight: weld.weldSize,
				coreLength: weld.weldLength,
				coreVolume: weld.weldPlaces,
				coreArea: weld.weldSide === 'Both' ? 2 : 1,
				noOfCore: weld.noOfWeldPasses,
				coreWidth: weld.wireDia,
				coreShape: weld.weldType,
				// Merge process fields for the calculator
				weldPosition: sub?.weldPosition,
				hlFactor: sub?.tackWelds,
				formPerimeter: sub?.intermediateStops,
				formHeight: sub?.travelSpeed
			}
		})

		const effectiveWeldLength = cuttingLength || totalWeldLength

		// ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
		// 5Ô∏è‚É£ Machine Master
		// ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
		const [ratedPower, powerUtilization, powerESG] = await Promise.all([
			this.page.safeGetNumber(this.page.RatedPower),
			this.page.safeGetNumber(this.page.PowerUtil),
			this.page.safeGetNumber(this.page.CO2PerKwHr)
		])

		// ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
		// 6Ô∏è‚É£ DTO & Process Mapping
		// ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
		const processMapping: Record<number, number> = {
			[ProcessType.MigWelding]: PrimaryProcessType.MigWelding,
			[ProcessType.TigWelding]: PrimaryProcessType.TigWelding,
			[ProcessType.SpotWelding]: PrimaryProcessType.SpotWelding,
			[ProcessType.SeamWelding]: PrimaryProcessType.SeamWelding,
			[ProcessType.StickWelding]: PrimaryProcessType.StickWelding
		}

		return {
			processTypeID: processType,
			partComplexity,
			machineHourRate,
			lowSkilledLaborRatePerHour,
			skilledLaborRatePerHour,
			noOfLowSkilledLabours,
			noOfSkilledLabours,
			qaOfInspectorRate,
			inspectionTime,
			samplingRate,
			electricityUnitCost,
			powerConsumption,
			yieldPer,
			lotSize,
			annualVolume,
			setUpTime,
			netMaterialCost,
			netPartWeight,
			cycleTime,
			totalWeldLength: effectiveWeldLength,
			cuttingLength: effectiveWeldLength,
			travelSpeed,
			UnloadingTime,
			semiAutoOrAuto,
			dryCycleTime,
			selectedVoltage,
			RequiredVoltage,
			passesLocator,
			MachineEfficiency: machineEfficiencyUI || machineEfficiency * 100,
			efficiency: machineEfficiencyUI || machineEfficiency * 100,
			materialInfoList: [{
				processId: processMapping[processType] || processType,
				density,
				netWeight: netPartWeight,
				netMatCost: netMaterialCost,
				partProjectedArea,
				weldLegLength,
				weldElementSize,
				totalWeldLength: effectiveWeldLength,
				dimX: length,
				dimY: width,
				dimZ: height,
				coreCostDetails,

			}],

			materialmasterDatas: {
				materialType: { materialTypeName }
			} as any,

			machineMaster: {
				machineHourRate,
				powerConsumption,
				efficiency: machineEfficiency * 100,
				totalPowerKW: ratedPower,
				powerUtilization: powerUtilization / 100
			},

			laborRates: [{ powerESG }],

			iscoolingTimeDirty: false,
			iscycleTimeDirty: false,
			isdirectMachineCostDirty: false,
			isdirectLaborCostDirty: false,
			isinspectionCostDirty: false,
			isdirectSetUpCostDirty: false,
			isyieldCostDirty: false
		}
	}

	private getWeldingCalculationStrategy(
		processType: ProcessType
	): WeldingCalcStrategy {
		switch (processType) {
			case ProcessType.WeldingCleaning:
				return dto => {
					this.calculator.calculationsForWeldingCleaning(dto, [], dto);
					return dto as unknown as Record<string, number>;
				};

			case ProcessType.WeldingPreparation:
				return dto => {
					this.calculator.calculationsForWeldingPreparation(dto, [], dto);
					return dto as unknown as Record<string, number>;
				};

			default:
				return dto => {
					this.calculator.calculationForWelding(dto, [], dto, []);
					return dto as unknown as Record<string, number>;
				};
		}
	}
	private async verifyWeldingCostsUnified(
		processType: ProcessType,
		options: {
			verifyTotal?: boolean;
			verifyCycleTime?: boolean;
			precision?: number;
		} = {}
	): Promise<Record<string, number>> {
		logger.info(`\nüí∞ Unified Welding Cost Verification ‚Üí ${ProcessType[processType]}`);

		const { density } =
			(await this.getMaterialDimensionsAndDensity()) ?? { density: 7.85 };

		const efficiency = (await this.page.getInputValueAsNumber(this.page.MachineEfficiency)) / 100;

		// Ensure UI ready (important for Cleaning/Prep)
		if (
			[ProcessType.WeldingCleaning, ProcessType.WeldingPreparation].includes(processType)
		) {
			await this.page.PartInformationTitle.scrollIntoViewIfNeeded();
			await this.page.wait(300);
		}

		// Build DTO
		const manufactureInfo = await this.gatherManufacturingInfo(
			processType,
			efficiency,
			density
		);

		// Apply common defaults
		this.calculator.weldingPreCalc(manufactureInfo, [], manufactureInfo);

		// Calculate via strategy
		const calculate = this.getWeldingCalculationStrategy(processType);
		const calculated = calculate(manufactureInfo);

		// Verification matrix
		const verifications = [
			{
				label: 'Cycle Time',
				locator: this.page.CycleTimePart.first(),
				calc: manufactureInfo.cycleTime,
				enabled: options.verifyCycleTime
			},
			{
				label: 'Direct Machine Cost',
				locator: this.page.DirectMachineCost.first(),
				calc: calculated.directMachineCost
			},
			{
				label: 'Direct Labor Cost',
				locator: this.page.DirectLaborCost.first(),
				calc: calculated.directLaborCost
			},
			{
				label: 'Direct Setup Cost',
				locator: this.page.DirectSetUpCost.first(),
				calc: calculated.directSetUpCost
			},
			{
				label: 'QA Inspection Cost',
				locator: this.page.QAInspectionCost.first(),
				calc: calculated.inspectionCost ?? calculated.qaInspectionCost
			},
			{
				label: 'Power Cost',
				locator: this.page.TotalPowerCost.first(),
				calc: calculated.powerCost ?? calculated.totalPowerCost
			},
			{
				label: 'Yield Cost',
				locator: this.page.YieldCostPart.first(),
				calc: calculated.yieldCost
			}
		];

		let totalCalculated = 0;

		for (const v of verifications) {
			if (v.enabled === false) continue;

			const expected = Number(v.calc) || 0;
			if (expected === 0) continue;

			await this.page.verifyUIValue({
				locator: v.locator,
				expectedValue: expected,
				label: v.label,
				precision: options.precision ?? 4
			});

			totalCalculated += expected;
		}

		// Total Manufacturing Cost
		if (options.verifyTotal && totalCalculated > 0) {
			await this.page.verifyUIValue({
				locator: this.page.NetProcessCost,
				expectedValue: totalCalculated,
				label: 'Total Manufacturing Cost'
			});
		}

		return calculated;
	}



